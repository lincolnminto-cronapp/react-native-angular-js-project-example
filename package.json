{
  "name": "CSSModulesExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint . && stylelint 'src/**/*.@(css|scss)'",
    "web": "expo start --web",
    "web2": "webpack-dev-server --mode development",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@types/angular": "^1.8.4",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "angular": "^1.8.2",
    "angular-ui-router": "1.0.29",
    "angular2react": "^3.0.2",
    "cronapi-js": "^2.8.22",
    "cronapp-common-js": "^2.8.13",
    "cronapp-framework-js": "^2.8.37",
    "cronapp-lib-js": "^2.8.6",
    "dangerously-set-html-content": "^1.0.9",
    "es6-shim": "^0.35.5",
    "expo": "~44.0.2",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "react": "16.9.0",
    "react-art": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-fontawesome": "^1.6.1",
    "react-native": "0.61.5",
    "react-native-fontawesome": "^5.7.0",
    "react-native-web": "^0.12.2"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.9.0",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@teamthread/strict-css-modules-loader": "^0.0.3",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-native-classname-to-style": "^1.2.2",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-preset-react": "^7.0.0-beta.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "file-loader": "^3.0.1",
    "html-loader": "^1.3.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "node-sass": "^4.12.0",
    "patch-package": "^6.2.1",
    "postcss": "^7.0.16",
    "postcss-css-variables": "^0.12.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "3.1.3",
    "react-native-postcss-transformer": "^1.2.4",
    "react-native-sass-transformer": "^1.4.0",
    "react-test-renderer": "16.9.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-react-native-css-modules": "^3.0.0",
    "stylelint-react-native": "^2.1.1",
    "typescript": "^2.8.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "jest": {
    "preset": "react-native"
  },
  "postcss": {
    "plugins": {
      "postcss-css-variables": {}
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "css-modules"
    ],
    "extends": [
      "@react-native-community",
      "plugin:css-modules/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
